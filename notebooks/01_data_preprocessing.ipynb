   "id": "79393606",
   "metadata": {},
   "source": [
    "# Setup & Paths"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "466cd067",
   "metadata": {},
   "outputs": [],
   "source": [
    "# --- Setup ---\n",
    "import pandas as pd\n",
    "import numpy as np\n",
    "from pathlib import Path\n",
    "\n",
    "from _init_path import *\n",
    "\n",
    "# --- Paths ---\n",
    "base = Path.cwd().parent          # assumes this notebook is in /notebooks\n",
    "raw_path = base / \"data\" / \"raw\"\n",
    "processed_path = base / \"data\" / \"processed\"\n",
    "processed_path.mkdir(parents=True, exist_ok=True)"
   ]
  },
  {
   "cell_type": "markdown",
   "id": "a5bf0749",
   "metadata": {},
   "source": [
    "# Load Raw Data"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "dc0fe462",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Raw main shape: (40949, 16)\n",
      "Raw subs shape: (4547, 23)\n"
     ]
    }
   ],
   "source": [
    "# --- Load raw data ---\n",
    "df_raw = pd.read_csv(raw_path / \"USvideos.csv\")\n",
    "df_subs = pd.read_csv(raw_path / \"USvideos_with_subscribers.csv\")\n",
    "\n",
    "print(\"Raw main shape:\", df_raw.shape)\n",
    "print(\"Raw subs shape:\", df_subs.shape)"
   ]
  },
  {
   "cell_type": "markdown",
   "id": "f36708e7",
   "metadata": {},
   "source": [
    "# Basic Cleaning & Types"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "11a2a67b",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "After core cleaning: (40949, 16)\n"
     ]
    }
   ],
   "source": [
    "# --- Clean numeric types ---\n",
    "for col in [\"views\", \"likes\", \"dislikes\", \"comment_count\"]:\n",
    "    df_raw[col] = pd.to_numeric(df_raw[col], errors=\"coerce\")\n",
    "\n",
    "# --- Timestamps ---\n",
    "df_raw[\"publish_time\"] = pd.to_datetime(df_raw[\"publish_time\"], errors=\"coerce\")\n",
    "\n",
    "# --- Drop rows missing core info ---\n",
    "core_cols = [\"video_id\", \"title\", \"channel_title\", \"views\", \"likes\", \"comment_count\", \"publish_time\"]\n",
    "df_raw = df_raw.dropna(subset=core_cols)\n",
    "\n",
    "print(\"After core cleaning:\", df_raw.shape)"
   ]
  },
  {
   "cell_type": "markdown",
   "id": "6bba5fd2",
   "metadata": {},
   "source": [
    "# Merge Subscribers"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "id": "0692027b",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "After merge shape: (40949, 17)\n",
      "Missing subscribers %: 44.8\n"
     ]
    }
   ],
   "source": [
    "# --- Keep only needed cols from subscribers file ---\n",
    "df_subs_small = df_subs[[\"video_id\", \"subscriber\"]].copy()\n",
    "\n",
    "# --- Merge on video_id ---\n",
    "df = pd.merge(\n",
    "    df_raw,\n",
    "    df_subs_small,\n",
    "    on=\"video_id\",\n",
    "    how=\"left\"\n",
    ")\n",
    "\n",
    "print(\"After merge shape:\", df.shape)\n",
    "print(\"Missing subscribers %:\",\n",
    "      round(df[\"subscriber\"].isna().mean() * 100, 2))\n",
    "\n",
    "# Rename column for clarity\n",
    "df = df.rename(columns={\"subscriber\": \"subscribers\"})\n"
   ]
  },
  {
   "cell_type": "markdown",
   "id": "a9b2a3f3",
   "metadata": {},
   "source": [
    "# Deduplicate by Video & Impute Subscribers"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "id": "66da49f8",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Deduped by video_id: 40949 -> 6351\n",
      "After subscriber imputation/drop: (5905, 17)\n"
     ]
    }
   ],
   "source": [
    "# --- Deduplicate: keep row with max views per video_id ---\n",
    "before = len(df)\n",
    "df = df.loc[df.groupby(\"video_id\")[\"views\"].idxmax()].copy()\n",
    "after = len(df)\n",
    "\n",
    "print(\"Deduped by video_id: {} -> {}\".format(before, after))\n",
    "\n",
    "# --- Impute subscribers with channel-level mean ---\n",
    "df[\"subscribers\"] = df.groupby(\"channel_title\")[\"subscribers\"].transform(\n",
    "    lambda x: x.fillna(x.mean())\n",
    ")\n",
    "\n",
    "# --- Drop rows where subscribers still missing ---\n",
    "df = df.dropna(subset=[\"subscribers\"])\n",
    "print(\"After subscriber imputation/drop:\", df.shape)"
   ]
  },
  {
   "cell_type": "markdown",
   "id": "8e283f12",
   "metadata": {},
   "source": [
    "# Compute Views-per-Subscriber & Filter Outliers"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "id": "f0bf4f35",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "VPS before filtering:\n",
      "count    5.905000e+03\n",
      "mean     1.514027e+04\n",
      "std      3.357623e+05\n",
      "min      9.167398e-04\n",
      "50%      3.209575e-01\n",
      "75%      1.123944e+00\n",
      "90%      8.880897e+00\n",
      "99%      5.899488e+04\n",
      "max      1.966910e+07\n",
      "Name: views_per_subscriber, dtype: float64\n",
      "After filters (subs >= 50, VPS <= 1000): (5742, 18)\n",
      "Final VPS log stats:\n",
      "count    5742.000000\n",
      "mean        0.666625\n",
      "std         1.078639\n",
      "min         0.000916\n",
      "25%         0.098410\n",
      "50%         0.265276\n",
      "75%         0.681627\n",
      "max         6.908745\n",
      "Name: views_per_subscriber_log, dtype: float64\n"
     ]
    }
   ],
   "source": [
    "# --- Compute views_per_subscriber ---\n",
    "df[\"views_per_subscriber\"] = df[\"views\"] / (df[\"subscribers\"] + 1)\n",
    "\n",
    "# --- Basic stats before filtering ---\n",
    "print(\"VPS before filtering:\")\n",
    "print(df[\"views_per_subscriber\"].describe(percentiles=[0.5, 0.75, 0.9, 0.99]))\n",
    "\n",
    "# --- Filter: remove tiny channels and extreme VPS outliers ---\n",
    "min_subscribers = 50          # you can tweak if needed\n",
    "max_vps = 1000                # cap extreme ratios\n",
    "\n",
    "mask = (df[\"subscribers\"] >= min_subscribers) & (df[\"views_per_subscriber\"] <= max_vps)\n",
    "df = df[mask].copy()\n",
    "\n",
    "print(\"After filters (subs >= {}, VPS <= {}):\".format(min_subscribers, max_vps), df.shape)\n",
    "\n",
    "# --- Log-transform for future regression target ---\n",
    "df[\"views_per_subscriber_log\"] = np.log1p(df[\"views_per_subscriber\"])\n",
    "\n",
    "\n",
    "print(\"Final VPS log stats:\")\n",
    "print(df[\"views_per_subscriber_log\"].describe())\n"
   ]
  },
